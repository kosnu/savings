{
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/cli/parse-args": "jsr:@std/cli@1/parse-args",
    "@std/csv": "jsr:@std/csv@1",
    "@std/dotenv/load": "jsr:@std/dotenv@0.224.0/load"
  },
  "fmt": {
    "indentWidth": 2,
    "singleQuote": false,
    "proseWrap": "preserve",
    "semiColons": false
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": ["no-unused-vars"]
    }
  }
}
