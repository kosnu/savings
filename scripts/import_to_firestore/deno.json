{
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "fmt": {
    "indentWidth": 2,
    "singleQuote": false,
    "proseWrap": "preserve",
    "semiColons": false
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": ["no-unused-vars"]
    }
  }
}
